programa
{
	cadeia mae = "Tess"
	cadeia filha = "Anna"
	cadeia troca
	funcao inicio()
	{
		escreva(mae," ",filha)
		troca = mae
		mae = filha
		filha = troca
		escreva("\n",mae," ",filha)
	}
}
